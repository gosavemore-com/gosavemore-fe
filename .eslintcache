>\n          <Row>\n            <Col span={12}>\n              <Form.Item\n                label=\"Address\"\n                name=\"address\"\n                rules={[\n                  {\n                    required: true,\n                    message: \"Address!\",\n                  },\n                ]}\n              >\n                <Input className=\"shipping-delivery-form-input\" />\n              </Form.Item>\n            </Col>\n            <Col span={12}>\n              <Form.Item\n                label=\"City\"\n                name=\"city\"\n                rules={[\n                  {\n                    required: true,\n                    message: \"City\",\n                  },\n                ]}\n              >\n                <Input className=\"shipping-delivery-form-input\" />\n              </Form.Item>\n            </Col>\n          </Row>\n          <Row>\n            <Col span={12}>\n              <Form.Item label=\"State\" name=\"state\" required>\n                <Select>\n                  {Object.keys(states).map((state, index) => (\n                    <Select.Option value={state} key={index}>\n                      {states[state]}\n                    </Select.Option>\n                  ))}\n                </Select>\n              </Form.Item>\n            </Col>\n            <Col span={12}>\n              <Form.Item\n                label=\"Postal\"\n                name=\"postal\"\n                rules={[\n                  {\n                    required: true,\n                    message: \"Postal\",\n                  },\n                ]}\n              >\n                <Input />\n              </Form.Item>\n            </Col>\n          </Row>\n          <Row>\n            <Col span={12}>\n              <Form.Item label=\"Country\" name=\"country\" required>\n                <Select>\n                  <Select.Option value={\"US\"}>United States</Select.Option>\n                </Select>\n              </Form.Item>\n            </Col>\n          </Row>\n          <Row justify=\"space-around\" align=\"middle\">\n            <Col>\n              <Button\n                type=\"primary\"\n                className=\"shipping-submit\"\n                htmlType=\"submit\"\n              >\n                Use this address\n              </Button>\n            </Col>\n          </Row>\n        </Form>\n      </div>\n    </>\n  );\n};\n\nexport default Shipping;\n","/Users/denmercs/Desktop/Project Files/gosavemore-fe/src/pages/Payment.jsx",["225","226"],"import React, { useState, useEffect } from \"react\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { useHistory } from \"react-router-dom\";\nimport CheckoutSteps from \"../components/CheckoutSteps\";\nimport { Button, Radio, Row, Col } from \"antd\";\nimport { savePaymentDetails } from \"../redux/actions/cartAction\";\n\nconst Payment = () => {\n  const [value, setValue] = useState(\"paypal\");\n  const { paymentMethod } = useSelector((state) => state.cart.payments);\n  const history = useHistory();\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n    if (paymentMethod !== undefined) {\n      setValue(paymentMethod);\n    }\n  }, [paymentMethod]);\n\n  const onChange = (e) => {\n    setValue(e.target.value);\n  };\n\n  const handleSubmit = () => {\n    dispatch(savePaymentDetails({ paymentMethod: value }));\n    history.push(\"/placeorder\");\n  };\n\n  return (\n    <>\n      <CheckoutSteps step1=\"finish\" step2=\"process\" step3=\"wait\" step4=\"wait\" />\n      <div className=\"payment\">\n        <div className=\"payment-choices\">\n          <h3>Payment Choices</h3>\n          <Radio.Group onChange={onChange} value={value}>\n            <Radio value=\"paypal\">Paypal</Radio>\n            {/* <Radio value=\"stripe\">Stripe </Radio> */}\n          </Radio.Group>\n        </div>\n        <div className=\"payment-button\">\n          <Button type=\"primary\" onClick={() => history.push(\"/shipping\")}>\n            Back\n          </Button>\n          <Button type=\"primary\" onClick={handleSubmit}>\n            Continue\n          </Button>\n        </div>\n      </div>\n    </>\n  );\n};\n\nexport default Payment;\n","/Users/denmercs/Desktop/Project Files/gosavemore-fe/src/pages/Profile.jsx",[],"/Users/denmercs/Desktop/Project Files/gosavemore-fe/src/pages/ProductDetails.jsx",[],"/Users/denmercs/Desktop/Project Files/gosavemore-fe/src/pages/Order.jsx",["227"],"import React, { useState, useEffect } from \"react\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { PayPalButton } from \"react-paypal-button-v2\";\nimport OrderDetails from \"../components/OrderDetails\";\nimport {\n  fetchOrderById,\n  resetOrderPay,\n  updateOrderPay,\n} from \"../redux/actions/orderAction\";\nimport Spinner from \"../components/Spinner\";\nimport { clearCart, clearOrders } from \"../redux/actions/cartAction\";\nimport { Button } from \"antd\";\nimport { useHistory } from \"react-router-dom\";\n\nconst Order = ({ match }) => {\n  const orderId = match.params.id;\n  const dispatch = useDispatch();\n  const history = useHistory();\n  const [tableData, setTableData] = useState([]);\n\n  const {\n    shippingAddress,\n    orderItems,\n    taxPrice,\n    shippingPrice,\n    paymentMethod,\n    totalPrice,\n    isDelivered,\n    isPaid,\n  } = useSelector((state) => state.orders.ordered);\n\n  const { user } = useSelector((state) => state.users);\n\n  const { isSuccess, isPaymentProcessingSuccess } = useSelector(\n    (state) => state.orders\n  );\n\n  useEffect(() => {\n    setTableData(orderItems);\n  }, [orderItems]);\n\n  useEffect(() => {\n    if (isPaymentProcessingSuccess) {\n      dispatch(fetchOrderById(orderId));\n      dispatch(clearCart());\n      dispatch(clearOrders());\n    }\n  }, [dispatch, isPaymentProcessingSuccess, orderId]);\n\n  const successPaymentHandler = (paymentResult) => {\n    dispatch(updateOrderPay(orderId, paymentResult));\n  };\n\n  const handleClick = () => {\n    dispatch(resetOrderPay());\n    history.push(\"/\");\n  };\n\n  return (\n    <>\n      <div className=\"order\">\n        <h2>Order id: {orderId}</h2>\n        <div className=\"order-details\">\n          <OrderDetails\n            tableData={tableData}\n            {...shippingAddress}\n            taxPrice={taxPrice}\n            shippingPrice={shippingPrice}\n            totalPrice={totalPrice}\n            paymentMethod={paymentMethod}\n            user={user}\n            isDelivered={isDelivered}\n            isPaid={isPaid}\n          />\n          <div className=\"order-buttons\">\n            {!isPaid && (\n              <PayPalButton\n                amount={totalPrice}\n                onSuccess={successPaymentHandler}\n              />\n            )}\n\n            {isPaid && isSuccess && (\n              <Button type=\"primary\" onClick={handleClick}>\n                Home\n              </Button>\n            )}\n          </div>\n        </div>\n      </div>\n    </>\n  );\n};\n\nexport default Order;\n","/Users/denmercs/Desktop/Project Files/gosavemore-fe/src/pages/PlaceOrder.jsx",[],"/Users/denmercs/Desktop/Project Files/gosavemore-fe/src/pages/UserList.jsx",[],"/Users/denmercs/Desktop/Project Files/gosavemore-fe/src/redux/reducers/userReducer.js",[],"/Users/denmercs/Desktop/Project Files/gosavemore-fe/src/redux/reducers/cartReducer.js",[],"/Users/denmercs/Desktop/Project Files/gosavemore-fe/src/redux/reducers/productsReducer.js",[],"/Users/denmercs/Desktop/Project Files/gosavemore-fe/src/redux/reducers/ordersReducer.js",[],"/Users/denmercs/Desktop/Project Files/gosavemore-fe/src/util/pricingCalculation.js",[],"/Users/denmercs/Desktop/Project Files/gosavemore-fe/src/util/data.js",[],"/Users/denmercs/Desktop/Project Files/gosavemore-fe/src/redux/actions/userAction.js",[],"/Users/denmercs/Desktop/Project Files/gosavemore-fe/src/redux/actions/cartAction.js",[],"/Users/denmercs/Desktop/Project Files/gosavemore-fe/sr